{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "draggable-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/draggable/DraggableWebPart.js",
          "manifest": "./src/webparts/draggable/DraggableWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": "node_modules/jquery/dist/jquery.min.js",
    "jqueryui": "node_modules/jqueryui/jquery-ui.min.js"
  },
  "localizedResources": {
    "DraggableWebPartStrings": "lib/webparts/draggable/loc/{locale}.js"
  }
}
